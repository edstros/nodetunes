<!DOCTYPE html>
<html lang="en">
<head><link rel="icon" href="data:;base64,iVBORw0KGgo=">
  <meta charset="UTF-8">
  <title><%= title %> | artists</title>
</head>
<body>
 <h1>Artists</h1>
 <a href="/artists/addartist">Add new artist</a>
  <table>
<thead><tr>
  <td>Name</td>
  <td>Genre</td>
  <td>Language</td>
  <td>Nationality</td>
  <td>Bio</td>
  <td>Members</td>
  <td>Wiki</td>
</tr>  <tbody><% artists.forEach(function (artist){ %>
     <tr>
      <td><%= artist.name %></td>
      <td><%= artist.genre %></td>
      <td><%= artist.language %></td>
      <td><%= artist.nationality %></td>
      <td><%= artist.bio %></td>
      <td><%= artist.members %></td>
      <td><%= artist.wiki %></td>
    </tr>
      <% }); %>
  </tbody></thead>
  </table>
</body>
</html>
